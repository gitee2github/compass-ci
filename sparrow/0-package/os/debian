export DEBIAN_FRONTEND=noninteractive

pkgs=(
	vim
	lvm2
	gcc
	g++
	make
	docker.io
	ruby-full
	ruby-dev
	qemu
	dmidecode
	lockfile-progs
	bridge-utils
	busybox
)

apt-get install -y --fix-missing "${pkgs[@]}"
