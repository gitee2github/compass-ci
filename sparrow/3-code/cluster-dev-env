. $CCI_SRC/container/defconfig.sh

load_service_config

cat > $CCI_SRC/container/dnsmasq/dnsmasq.d/${lab}.conf <<EOF
interface=$interface
dhcp-authoritative
dhcp-range=$dhcp_range
EOF
