. $CCI_SRC/container/defconfig.sh
docker_rm srv-http-result

cmd=(
	docker run
	--restart=always
	--name srv-http-result
	-p 11300:11300
	-v /srv/result:/srv/result:ro
	-v /etc/localtime:/etc/localtime:ro
	-d
	srv-http
)

"${cmd[@]}"
